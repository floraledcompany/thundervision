{
  "name": "thundervision",
  "version": "1.0.0",
  "productName": "Thundervision",
  "description": "A lightweight project starter for the Thundervision team at the Denver Broncos",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/thundervision_icon_icns/1024x1024.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/thudnervision_icon_ico/1024x1024.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/thundervision_icon_pngs/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [
    "thundervision",
    "project",
    "start"
  ],
  "author": "Lucas Flora",
  "license": "ISC",
  "dependencies": {
    "delay": "^4.1.0",
    "electron": "^4.0.1",
    "electron-settings": "^3.2.0",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "electron-packager": "^13.0.1",
    "wine": "^0.9.8"
  }
}
